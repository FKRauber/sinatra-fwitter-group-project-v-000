<h2>Please Login or Sign Up</h2>
<nav>
  <% if logged_in? %>
    <p>Welcome, <%= current_user.username %></p>
    <a href='/tweets/new'>Create New Tweet</a>
    <a href='/logout'>Log Out</a>
  <% else %>
    <h3><a href='./users/login'>Login</a></h3>
    <h3><a href='./users/create_user'>Sign Up</a></h3>
  <% end %>
</nav>
